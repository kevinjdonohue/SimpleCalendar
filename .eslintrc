{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": false,
    "shallow": false,
    "React": false
  },
  "rules": {
    "max-len": [2, { "code": 120 }],
    "no-underscore-dangle": [2, { "allowAfterThis": true }]
  }
}
